{
  "name": "openapi-client-sdk-loader",
  "version": "0.1.1",
  "description": "Load client sdk from Open API documentation",
  "license": "MIT",
  "repository": "git@github.com:aleksandryackovlev/openapi-client-sdk-loader.git",
  "author": "Aleksandr Yackovlev <aleksandryackovlev@yandex.ru>",
  "homepage": "https://github.com/aleksandryackovlev/openapi-client-sdk-loader",
  "bugs": "https://github.com/aleksandryackovlev/openapi-client-sdk-loader/issues",
  "main": "dist/cjs.js",
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "build": "NODE_ENV=production babel src -d dist --copy-files",
    "security": "npm audit",
    "lint": "eslint .",
    "test:only": "NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "test": "npm run test:coverage",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "files": [
    "dist/",
    "lib/",
    "index.js"
  ],
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "glob": "^7.1.6",
    "handlebars": "^4.5.3",
    "json-schema-to-typescript": "^8.0.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.15",
    "query-string": "^6.9.0",
    "schema-utils": "^2.6.1",
    "swagger-parser": "^8.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-jest": "^24.9.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "loader-runner": "^3.1.0",
    "memfs": "^3.0.1",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2"
  },
  "keywords": [
    "webpack"
  ],
  "jest": {
    "testMatch": [
      "**/(src|test)/**/*.(spec|test).js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/process-staged-files"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
